<Window x:Name="ConfigurationsTuningWindow" x:Class="PFE.ConfigsTuningWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PFE"
        mc:Ignorable="d"
        Title="Configurations d'accordage" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="btn_ok" Content="Ok" HorizontalAlignment="Left" Margin="222,238,0,0" VerticalAlignment="Top" Width="60" Click="btn_ok_Click"/>
        <Label x:Name="label" Content="Accordage :" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <StackPanel HorizontalAlignment="Left" Height="165" Margin="10,36,0,0" VerticalAlignment="Top" Width="272">
            <ComboBox x:Name="cb_note" VerticalAlignment="Top" RenderTransformOrigin="0.755,0.775" HorizontalAlignment="Left" Width="120" SelectedIndex="0"/>
        </StackPanel>
    </Grid>
</Window>
